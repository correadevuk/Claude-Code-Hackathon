# SlideRx N8N Environment Variables
# NOTE: Most credentials are configured in N8N UI (http://localhost:5678 â†’ Credentials)
# This file shows optional environment variables for advanced configuration

# N8N Configuration (set in docker-compose.yml)
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=sliderx123
N8N_PROTOCOL=http
N8N_SECURE_COOKIE=false

# ngrok Webhook URL (update after starting ngrok)
WEBHOOK_URL=https://your-ngrok-url.ngrok-free.dev/

# Frontend Configuration (optional - currently uses mock data)
VITE_API_URL=https://your-api-gateway-url.amazonaws.com

# AWS Credentials (configure these in N8N UI instead)
# AWS S3 Credential Name: "AWS S3 SlideRx"
# - Access Key ID: your_access_key_id
# - Secret Access Key: your_secret_access_key
# - Region: eu-central-1

# OpenRouter API (configure in N8N UI instead)
# Credential Name: "OpenRouter API"
# - Type: HTTP Header Auth
# - Header: Authorization
# - Value: Bearer your_openrouter_api_key

# Development Settings
NODE_ENV=development
FRONTEND_PORT=3000
