{
  "name": "AWS S3 Test - List Objects",
  "nodes": [
    {
      "parameters": {},
      "id": "manual-trigger",
      "name": "Manual Trigger",
      "type": "n8n-nodes-base.manualTrigger",
      "typeVersion": 1,
      "position": [240, 300]
    },
    {
      "parameters": {
        "operation": "getAll",
        "bucketName": "sliderx-uploads-dev",
        "returnAll": true,
        "options": {}
      },
      "id": "list-s3-objects",
      "name": "List S3 Objects",
      "type": "n8n-nodes-base.awsS3",
      "typeVersion": 1,
      "position": [460, 300],
      "credentials": {
        "aws": {
          "id": "PLACEHOLDER",
          "name": "AWS S3 SlideRx"
        }
      }
    },
    {
      "parameters": {
        "functionCode": "// Display the results\nconst items = $input.all();\n\nif (items.length === 0) {\n  return [{\n    json: {\n      success: true,\n      message: \"Bucket is empty or accessible\",\n      objectCount: 0\n    }\n  }];\n}\n\nreturn [{\n  json: {\n    success: true,\n    message: \"Successfully connected to S3!\",\n    objectCount: items.length,\n    firstObject: items[0].json.Key || 'Unknown',\n    allObjects: items.map(item => item.json.Key || item.json)\n  }\n}];"
      },
      "id": "format-result",
      "name": "Format Result",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [680, 300]
    }
  ],
  "connections": {
    "Manual Trigger": {
      "main": [
        [
          {
            "node": "List S3 Objects",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "List S3 Objects": {
      "main": [
        [
          {
            "node": "Format Result",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "settings": {
    "executionOrder": "v1"
  }
}
